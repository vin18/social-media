    "colors": "^1.4.0",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "react-icons": "^4.3.1"
  },
  "type": "module",
  "devDependencies": {
    "concurrently": "^7.0.0"
  }
}
